= content_for :actions do
  = link_to votes_history_profile_url do
    = image_tag "list-ordered.png"
    Voting History

- if @project_board_votes.empty? && @dataset_votes.empty?
  %p Currently there are no votes to give for you.

- unless @project_board_votes.empty?
  %h2 Project Board Votes
  = render :partial => 'open_votes_list', :locals => {:votes => @project_board_votes}

- unless @dataset_votes.empty?
  %h2 Data Request Votes
  = render :partial => 'open_votes_list', :locals => {:votes => @dataset_votes}