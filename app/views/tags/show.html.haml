%h1
  = @tag.name

%ul
- @tag.taggings.each do |tagging|
  %li
    = link_to tagging.taggable.dataset.title, {:controller => :datasets,
                                               :action => :show,
                                               :id => tagging.taggable.dataset_id }
    = "( #{tagging.taggable.long_label} )"

