
<% icat_object = @subm_list[ch].import_categories.select{|icat|
     icat.raw_data_value == icat_entry}[0]
   datagroup  = @subm_list[ch].methodstep
   dglist = datagroup.datacell_importcat_categories.collect{|cat|
     [cat.verbose, cat.id]}
   form_for icat_object, :url => {:action => :update_import_category_change_category,
     :controller => :import} do |form| %>
     <%= form.hidden_field :id %>
     <%= form.select(:categoricvalue_id, dglist) %>
     <%= form.submit %>
<% end %>
